{
	"name": "@awesome-whatthefar/parser",
	"version": "0.0.14",
	"main": "lib/index.js",
	"typings": "lib/index.d.ts",
	"license": "MIT",
	"scripts": {
		"prepublish": "yarn build",
		"prebuild": "rm -rf lib && rm -f tsconfig.build.tsbuildinfo",
		"build": "tsc -p tsconfig.build.json",
		"start": "ts-node src/index.ts",
		"start:dev": "nodemon",
		"start:debug": "nodemon --config nodemon-debug.json",
		"prestart:prod": "rimraf lib && yarn build",
		"start:prod": "node lib/index.js",
		"test": "jest --verbose",
		"test:watch": "jest --verbose --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
		"lint": "tslint -p tsconfig.json -c tslint.json",
		"lint:fix": "tslint -p tsconfig.json -c tslint.json --fix",
		"prettier": "prettier --config .prettierrc --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"format": "yarn prettier && yarn lint:fix"
	},
	"dependencies": {
		"axios": "^0.21.0",
		"dotenv": "^7.0.0",
		"lodash": "^4.17.11",
		"markdown-toc": "^1.2.0",
		"papaparse": "^4.6.2",
		"request": "^2.88.0",
		"tsconfig-paths": "^3.7.0"
	},
	"devDependencies": {
		"@types/axios": "^0.14.0",
		"@types/dotenv": "6.1.0",
		"@types/jest": "23.3.14",
		"@types/lodash": "4.14.123",
		"@types/papaparse": "4.5.9",
		"@types/prettier": "1.16.1",
		"@types/request": "2.48.1",
		"@types/request-promise-native": "1.0.15",
		"jest": "23.6.0",
		"prettier": "1.16.4",
		"prettierrc": "0.0.0",
		"rimraf": "2.6.3",
		"ts-jest": "24.0.0",
		"tslint": "5.14.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
