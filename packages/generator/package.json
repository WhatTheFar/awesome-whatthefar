{
	"name": "@awesome-whatthefar/generator",
	"version": "0.0.5",
	"main": "index.ts",
	"license": "MIT",
	"private": true,
	"scripts": {
		"start": "ts-node src/index.ts",
		"test": "jest --verbose",
		"test:watch": "jest --verbose --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
		"lint": "tslint -p tsconfig.json -c tslint.json",
		"lint:fix": "tslint -p tsconfig.json -c tslint.json --fix",
		"prettier": "prettier --config .prettierrc --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"format": "yarn prettier && yarn lint:fix"
	},
	"dependencies": {
		"@awesome-whatthefar/parser": "^0.0.5",
		"@types/glob": "^7.1.1",
		"front-matter": "^3.0.1",
		"glob": "^7.1.3"
	},
	"devDependencies": {
		"@types/jest": "^26.0.18",
		"jest": "^26.6.3",
		"prettier": "^2.2.1",
		"ts-jest": "^26.4.4",
		"tsconfig-paths": "^3.9.0"
	}
}
