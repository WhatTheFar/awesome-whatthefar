{
	"name": "awesome-whatthefar",
	"version": "1.0.0",
	"description": "ðŸ˜Ž Curated list of awesome WhatTheFar",
	"repository": "git@github.com:WhatTheFar/awesome-whatthefar.git",
	"author": "WhatTheFar <jakpat.m@gmail.com>",
	"license": "MIT",
	"scripts": {
		"awesome": "ts-node src/script/gen-readme.ts",
		"build": "tsc -p tsconfig.build.json",
		"start": "ts-node -r tsconfig-paths/register src/index.ts",
		"start:dev": "nodemon",
		"start:debug": "nodemon --config nodemon-debug.json",
		"prestart:prod": "rimraf dist && npm run build",
		"start:prod": "node -r ./tsconfig-paths-bootstrap.js dist/index.js",
		"test": "jest --verbose",
		"test:watch": "jest --verbose --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"lint": "tslint -p tsconfig.json -c tslint.json",
		"lint:fix": "tslint -p tsconfig.json -c tslint.json --fix",
		"prettier": "prettier --config .prettierrc --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"format": "yarn prettier && yarn lint:fix"
	},
	"dependencies": {
		"dotenv": "^6.2.0",
		"lodash": "^4.17.11",
		"papaparse": "^4.6.2",
		"request": "^2.88.0",
		"request-promise-native": "^1.0.5",
		"tsconfig-paths": "^3.7.0"
	},
	"devDependencies": {
		"@types/dotenv": "6.1.0",
		"@types/jest": "23.3.10",
		"@types/lodash": "4.14.119",
		"@types/papaparse": "4.5.5",
		"@types/prettier": "1.15.2",
		"@types/request": "2.48.1",
		"@types/request-promise-native": "1.0.15",
		"concurrently": "^4.1.0",
		"jest": "23.6.0",
		"nodemon": "1.18.9",
		"prettier": "1.15.3",
		"prettierrc": "0.0.0",
		"rimraf": "^2.6.3",
		"ts-jest": "23.10.5",
		"ts-node": "7.0.1",
		"tslint": "5.12.0",
		"tslint-microsoft-contrib": "6.0.0",
		"typescript": "3.2.2"
	}
}
